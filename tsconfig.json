{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "sourceMap": true
  }
}
